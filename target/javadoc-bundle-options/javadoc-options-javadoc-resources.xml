<?xml version="1.0" encoding="UTF-8"?>
<javadocOptions>
  <docletArtifacts>
    <docletArtifact />
    <docletArtifact>
      <groupId>org.jdrupes.mdoclet</groupId>
      <artifactId>doclet</artifactId>
      <version>3.1.0</version>
    </docletArtifact>
    <docletArtifact>
      <groupId>com.vladsch.flexmark</groupId>
